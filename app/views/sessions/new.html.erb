<div class="container">
<h1>Login</h1>
<body class= "body">
<table class="table">

 <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
 <tr>
   <div class="imgcontainer">
     <td class= "user"><%= image_tag("log.png") %> </td>
   </div>
</tr>
<tr>
  <td>
 <%= f.label :email %>
 <%= f.email_field :email %>
 </td></tr>
 <tr>
  <td> <%= f.label :password %>
       <%= f.password_field :password %></td></tr>
  <tr>
    <td> <%= f.submit "Log in" %></td></tr>
 <% end %>
</table>
